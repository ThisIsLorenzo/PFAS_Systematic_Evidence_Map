@ARTICLE{1551,
key={rayyan-789239997},
title={On the Dynamics of Electrons on Silver Single Crystal Surfaces, at Metal-Organic Interfaces and in PTCDA-Monolayers ; Über die Dynamik von Elektronen auf Silber-Einkristalloberflächen, an metall-organischen Grenzschichten und in PTCDA-Monolagen},
year={2013},
month={NA},
day={NA},
journal={},
issn={},
